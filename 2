import numpy as np
a1, a2 = np.array([5,6,7,8]), np.array([8,7,6,5])
print("Add:", np.add(a1,a2))
print("All non-zero:", np.all(a1))
print("Greater:", np.greater(a1,a2))
print("Equal:", np.equal(a1,a2))
print("Allclose:", np.allclose(a1,a2))
print("Zeros:", np.zeros(4))
print("Ones:", np.ones(4))
print("Linspace:", np.linspace(0,10,5))
