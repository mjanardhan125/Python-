#include <iostream>
#include <vector>
#include <list>
#include <map>
using namespace std;
int main(){
vector<int>v={1,2,3};v.push_back(4);
for(int x:v)cout<<x<<" ";
list<int>l={10,20};l.push_back(30);
for(int x:l)cout<<x<<" ";
map<int,string>m={{1,"One"},{2,"Two"}};
for(auto&p:m)cout<<p.first<<":"<<p.second<<" ";
}
